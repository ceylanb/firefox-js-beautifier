{

  "manifest_version": 2,
  "name": "JS Beautifier",
  "version": "1.0.0",
  "author": "Ceylan Bozogullarindan",
  "description": "Beautifies javascript files automatically...",

  "icons": {
    "48": "icons/jsbeautifier-48.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*/*.js", "*://*/*.js?*"],
      "css": ["assets/highlight/styles/monokai-sublime.css"],
      "js": ["assets/beautify.min.js" ,"assets/highlight/highlight.pack.js" ,  "beautifier.js"]
    }
  ]

}
